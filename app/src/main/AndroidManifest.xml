<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.AshishKumar.gujaratiquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.AshishKumar.gujaratiquiz.body4" />
        <activity android:name="com.AshishKumar.gujaratiquiz.body3" />
        <activity android:name="com.AshishKumar.gujaratiquiz.body2" />
        <activity android:name="com.AshishKumar.gujaratiquiz.level" />
        <activity android:name="com.AshishKumar.gujaratiquiz.about" />
        <activity android:name="com.AshishKumar.gujaratiquiz.body">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name="com.AshishKumar.gujaratiquiz.splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.AshishKumar.gujaratiquiz.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
    </application>

</manifest>